{
    "name": "ethicli",
    "version": "2.0",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "Navigates to index.html",
        "default_icon": "images/matcha.png"
    }
}